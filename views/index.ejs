		<!-- ONLY LOGO ON HEADER -->
		<div class="only-logo">
			<div class="navbar">
				<div class="navbar-header">
					<img src="../images/logo.png" alt="">
				</div>
			</div>
		</div> <!-- /END ONLY LOGO ON HEADER -->
		
		<div class="row">
			<div class="col-md-8 col-md-offset-2">
				
				<!-- HEADING AND BUTTONS -->
				<div class="intro-section">
					
					<!-- WELCOM MESSAGE -->
					<h1 class="intro">Es sólo una ayuda básica que necesita para corregir el resultado. </h1>
					<h5>Venga a compartir ideas, formar equipos, y nuevas empresas de lanzamiento.</h5>
					
					<!-- BUTTON -->
					<ul>
					<li class="buttons col-md-4" id="university-button">
						<a href="#" class="btn btn-default btn-lg standard-button"><i class="glyphicon glyphicon-map-marker"></i>Find a University</a>
					</li>

					<li class="buttons col-md-4" id="course-button">
						<a href="#" class="btn btn-default btn-lg standard-button"><i class="icon-app-download"></i>Find a Course</a>
					</li>
					<li class="buttons col-md-4" id="rate-button">
						<a href="#" class="btn btn-default btn-lg standard-button"><i class="icon_building_alt"></i>Rate a Course</a>
					</li>
					</ul>
					


					<!-- /END BUTTONS -->
					
				</div>
				<!-- /END HEADNING AND BUTTONS -->
				
			</div>
		</div>
		<!-- /END ROW -->
		
	</div>
	<!-- /END CONTAINER -->
</div>
