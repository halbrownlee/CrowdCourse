<!-- =========================
     FOOTER 
============================== -->
<footer>

<div class="container">
	
	<div class="contact-box wow rotateIn animated" data-wow-offset="10" data-wow-duration="1.5s">
		
		<!-- CONTACT BUTTON TO EXPAND OR COLLAPSE FORM -->
		
		<a class="btn contact-button expand-form expanded"><i class="icon_mail_alt"></i></a>
		
		<!-- EXPANDED CONTACT FORM -->
		<div class="row expanded-contact-form">
			
			<div class="col-md-6 ">
				
				<!-- FORM -->
				<form class="contact-form" id="Login" role="form" action="/users/login" method="post">
					
					<!-- IF MAIL SENT SUCCESSFULLY -->
					<h4 class="success">
						<i class="icon_check"></i> You have created your account.
					</h4>
					
					<!-- IF MAIL SENDING UNSUCCESSFULL -->
					<h4 class="error">
						<i class="icon_error-circle_alt"></i> Error!
					</h4>
					
					<div class="col-md-8 col-md-offset-2">
						<input class="form-control input-box" id="name" type="text" name="username" placeholder="User Name">
					</div>
					
					<!-- <div class="col-md-6">
						<input class="form-control input-box" id="email" type="email" name="email" placeholder="Email Address">
					</div> -->
					
					<div class="col-md-8 col-md-offset-2">
						<input class="form-control input-box" id="password" type="password" name="password" placeholder="Password">
					</div>

					<!-- <div class="col-md-12">
						<input class="form-control input-box" id="subject" type="text" name="subject" placeholder="Subject">
						<textarea class="form-control textarea-box" id="message" rows="8" placeholder="Message"></textarea>
					</div> -->
					<button class="btn btn-primary standard-button2 ladda-button" type="submit" id="submit" name="submit" data-style="expand-left">Login</button>
					
				</form>
				<!-- /END FORM -->
			</div>

			<div class="col-md-6">
				
				<!-- FORM -->
				<form class="contact-form" id="Login" role="form" action="/users/signup" method="post">
					
					<!-- IF MAIL SENT SUCCESSFULLY -->
					<h4 class="success">
						<i class="icon_check"></i> You have created your account.
					</h4>
					
					<!-- IF MAIL SENDING UNSUCCESSFULL -->
					<h4 class="error">
						<i class="icon_error-circle_alt"></i> Error!
					</h4>
					
					<div class="col-md-8 col-md-offset-2">
						<input class="form-control input-box" id="name" type="text" name="username" placeholder="User Name">
					</div>
					
					<!-- <div class="col-md-6">
						<input class="form-control input-box" id="email" type="email" name="email" placeholder="Email Address">
					</div> -->
					
					<div class="col-md-8 col-md-offset-2">
						<input class="form-control input-box" id="password" type="password" name="password" placeholder="Password">
					</div>

					<!-- <div class="col-md-12">
						<input class="form-control input-box" id="subject" type="text" name="subject" placeholder="Subject">
						<textarea class="form-control textarea-box" id="message" rows="8" placeholder="Message"></textarea>
					</div> -->
					<button class="btn btn-primary standard-button2 ladda-button" type="submit" id="submit" name="submit" data-style="expand-left">Sign up</button>
					
				</form>
				<!-- /END FORM -->
			</div>
			
		</div>
		<!-- /END EXPANDED CONTACT FORM -->
		
	</div>
	<!-- /END CONTACT BOX -->
	
	<!-- LOGO -->
	<img src="../images/logo.png" alt="LOGO" class="responsive-img">
	
	<!-- SOCIAL ICONS -->
